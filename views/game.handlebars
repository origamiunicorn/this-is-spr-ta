<script type="text/javascript" src="js/index.js"></script>
<div class="row">
    <div class="col s12">

        {{#each data}}
        <div class="row gamerow" style="background-image: url('/images/Clan{{dataValues.clanName}}SM.png');">
            <div class="col gameoffset">
                <div class="card" style="background: rgba(250, 250, 250, 0.7)">
                    <div class="card-content">
                        <h5 class="gameheader">{{dataValues.charName}}</h5>
                        <strong>of the {{dataValues.clanName}} clan</strong>
                        <i data-game-id="{{dataValues.id}}"
                            class="medium material-icons deleteGameBtn">delete_forever</i>

                        <div class="divider"></div>
                        <strong><a href="story/{{dataValues.lastStoryId}}">Continue Game</a></strong>
                    </div>
                </div>
            </div>
        </div>
        {{/each}}

        <a href="/">‚Üê Back To Home</a>
    </div>
</div>